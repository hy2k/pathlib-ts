{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"projectFolder": ".",
	"mainEntryPointFilePath": "<projectFolder>/dist/index.d.ts",
	"dtsRollup": {
		"enabled": false,
		"untrimmedFilePath": "<projectFolder>/docs/temp/index.d.ts"
	},
	"docModel": {
		"enabled": true,
		"apiJsonFilePath": "<projectFolder>/docs/temp/pathlib-ts.api.json"
	},
	"apiReport": {
		"enabled": false
	},
	"compiler": {
		"tsconfigFilePath": "<projectFolder>/tsconfig.build.json"
	}
}
